{
  "name": "@hive/core",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "pnpm run clean",
    "build": "tsup && tsc --emitDeclarationOnly --declaration --outDir dist",
    "lint": "cross-env BROWSERSLIST_IGNORE_OLD_DATA=1 NO_UPDATE_NOTIFIER=1 eslint .",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest"
  },
  "devDependencies": {
    "@hive/eslint-config": "workspace:*",
    "@hive/typescript-config": "workspace:*",
    "@types/node": "^20.12.1",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.0",
    "firebase": "^11.0.0",
    "firebase-admin": "^11.0.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "keywords": [],
  "author": "",
  "packageManager": "pnpm@9.1.1"
}
