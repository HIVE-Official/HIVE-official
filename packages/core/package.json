{
  "name": "@hive/core",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --sourcemap --clean --watch",
    "build": "node --max-old-space-size=4096 node_modules/.bin/tsup src/index.ts --format cjs,esm --sourcemap --clean --treeshake --minify --dts --dts-resolve",
    "lint": "eslint . --max-warnings 15",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest"
  },
  "devDependencies": {
    "@hive/eslint-config": "workspace:*",
    "@hive/typescript-config": "workspace:*",
    "@types/node": "^24.0.8",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "tsup": "^8.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.0",
    "firebase": "^11.0.0",
    "firebase-admin": "^11.0.0",
    "zod": "^3.25.67",
    "zustand": "^4.0.0"
  },
  "keywords": [],
  "author": "",
  "packageManager": "pnpm@9.1.1"
}
