{
  "name": "@hive/analytics",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rm -f tsconfig.tsbuildinfo && tsup src/index.ts --format esm,cjs --external react --sourcemap && tsc -p tsconfig.json --emitDeclarationOnly",
    "dev": "tsup src/index.ts --format esm,cjs --watch --external react --sourcemap & tsc -p tsconfig.json --watch --emitDeclarationOnly",
    "lint": "eslint src/ --max-warnings 15",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@hive/typescript-config": "workspace:*",
    "@types/react": "^18.2.0",
    "eslint": "^9.29.0",
    "tsup": "^6.7.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0"
  }
}
